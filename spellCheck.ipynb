    "from spellchecker import SpellChecker\n",
    "from textblob import TextBlob"
   ],
   "outputs": [],
   "metadata": {}
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "source": [
    "spell = SpellChecker()\n",
    "misspelled = spell.unknown([\"authrs\", \"watr\", \"study\", \"wrte\"])\n",
    "for word in misspelled:\n",
    "    print(spell.correction(word))"
   ],
   "outputs": [
    {
     "output_type": "stream",
